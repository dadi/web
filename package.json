{
  "name": "@dadi/web",
  "version": "6.0.1",
  "product": "DADI Web",
  "description": "A schemaless templating layer that can operate as a standalone platform or with DADI API as a full stack web application.",
  "main": "main.js",
  "scripts": {
    "start": "node ./start.js",
    "test": "standard 'lib/**/*.js' | snazzy && env NODE_ENV=test ./node_modules/.bin/istanbul cover -x '**/workspace/**' -x '**/app/**' --report cobertura --report text --report html --report lcov ./node_modules/mocha/bin/_mocha './test/{!(app),*}/*.js'",
    "posttest": "node ./scripts/coverage.js",
    "snyk-protect": "snyk protect",
    "prepublishOnly": "npm run snyk-protect",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "format": "prettier-standard --no-semi '{dadi,scripts,test}/**/*.js' && standard --fix '{dadi,scripts,test}/**/*.js'",
    "prepare": "npm run snyk-protect"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "dadi/**/*.js": [
      "prettier-standard --no-semi",
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@dadi/boot": "~1.1.3",
    "@dadi/cache": "~2.0.1",
    "@dadi/format-error": "~1.7.0",
    "@dadi/logger": "~1.4.1",
    "@dadi/metadata": "~2.0.0",
    "@dadi/passport": "~1.5.0",
    "@dadi/status": "~1.0.0",
    "@purest/providers": "~1.0.0",
    "async": "~1.5.2",
    "body-parser": "~1.18.2",
    "chokidar": "~2.0.0",
    "circular-json": "~0.5.1",
    "clone": "~2.1.1",
    "colors": "~1.1.2",
    "compressible": "~2.0.11",
    "connect-modrewrite": "~0.10.1",
    "connect-mongo": "~2.0.0",
    "connect-redis": "~3.3.0",
    "console-stamp": "~0.2.5",
    "convict": "~4.3.0",
    "cookie-parser": "~1.4.3",
    "csurf": "~1.9.0",
    "debug": "~2.6.9",
    "deepmerge": "~2.1.1",
    "etag": "~1.8.1",
    "event-stream": "~3.3.4",
    "express-session": "~1.15.6",
    "feedparser": "~2.2.2",
    "get-value": "~3.0.0",
    "iltorb": "~2.4.0",
    "js-yaml": "~3.12.0",
    "kinesis": "~1.2.2",
    "marked": "~0.3.9",
    "mime-types": "~2.1.17",
    "mongodb": "~3.1.0",
    "multer": "^1.3.0",
    "path-to-regexp": "~2.4.0",
    "perfy": "~1.1.2",
    "proxy-addr": "~1.1.5",
    "purest": "~3.1.0",
    "query-string": "~6.0.0",
    "recursive-readdir": "~2.2.1",
    "redis": "~2.8.0",
    "request": "2.88.0",
    "server-destroy": "~1.0.1",
    "snyk": "^1.88.1",
    "through": "~2.3.8",
    "toobusy-js": "~0.5.1",
    "uuid": "^3.1.0",
    "web-es6-templates": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.0",
    "@commitlint/config-conventional": "^7.1.1",
    "cheerio": "~1.0.0-rc.2",
    "env-test": "~1.0.0",
    "husky": "~0.13.3",
    "istanbul": "~0.4.5",
    "istanbul-cobertura-badger": "^1.1.0",
    "lint-staged": "~7.1.2",
    "mocha": "~3.2.0",
    "nock": "^9.1.10",
    "nodeunit": ">=0.5.1",
    "prettier-standard": "^8.0.1",
    "should": "~13.2.0",
    "sinon": "~2.3.2",
    "sinon-test": "~1.0.2",
    "snazzy": "~8.0.0",
    "standard": "8.6.0",
    "superagent": "~3.8.1",
    "superagent-mock": "~3.3.0",
    "supertest": "~3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dadi/web.git"
  },
  "bugs": {
    "url": "https://github.com/dadi/web/issues"
  },
  "homepage": "https://github.com/dadi/web",
  "directories": {
    "test": "test"
  },
  "standard": {
    "ignore": [
      "scripts",
      "test",
      "workspace"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "sinon",
      "mocha",
      "lint-staged"
    ]
  },
  "author": "DADI <team@dadi.tech>",
  "license": "SEE LICENSE IN GPL.md",
  "snyk": true
}
