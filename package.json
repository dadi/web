{
  "name": "@dadi/web",
  "version": "4.0.2",
  "description": "Web frontend and template layer for @dadi/api",
  "main": "main.js",
  "scripts": {
    "start": "node ./start.js",
    "test": "standard 'lib/**/*.js' | snazzy && env NODE_ENV=test ./node_modules/.bin/istanbul cover -x '**/workspace/**' -x '**/app/**' --report cobertura --report text --report html --report lcov ./node_modules/mocha/bin/_mocha test",
    "posttest": "node ./scripts/coverage.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "format": "prettier --no-semi --write '{dadi,scripts,test}/**/*.js' && standard --fix '{dadi,scripts,test}/**/*.js'"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --write",
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@dadi/cache": "~1.5.3",
    "@dadi/dustjs-helpers": "~1.6.0",
    "@dadi/format-error": "~1.5.0",
    "@dadi/logger": "~1.2.0",
    "@dadi/metadata": "~1.0.0",
    "@dadi/passport": "~1.5.0",
    "@dadi/status": "~1.0.0",
    "@dadi/web-dustjs": "~1.1.1",
    "@purest/providers": "~1.0.0",
    "async": "~1.5.2",
    "body-parser": "~1.18.2",
    "chokidar": "~1.7.0",
    "colors": "~1.1.2",
    "compressible": "~2.0.11",
    "connect-modrewrite": "~0.10.1",
    "connect-mongo": "~2.0.0",
    "connect-redis": "~3.3.0",
    "console-stamp": "~0.2.5",
    "convict": "~3.0.0",
    "cookie-parser": "~1.4.3",
    "csurf": "~1.9.0",
    "debug": "~2.6.9",
    "deepmerge": "~1.5.2",
    "dustjs-helpers": "~1.7.3",
    "dustjs-linkedin": "~2.7.5",
    "etag": "~1.8.1",
    "event-stream": "~3.3.4",
    "express-session": "~1.15.6",
    "feedparser": "~2.2.2",
    "fs-extra": "~3.0.1",
    "humanize-plus": "~1.8.2",
    "iltorb": "~1.3.10",
    "js-yaml": "~3.10.0",
    "kinesis": "~1.2.2",
    "marked": "~0.3.6",
    "mime-types": "~2.1.17",
    "mkdirp": "~0.5.1",
    "moment": "~2.19.1",
    "mongodb": "~2.2.0",
    "path-to-regexp": "~1.7.0",
    "perfy": "~1.1.2",
    "proxy-addr": "~1.1.5",
    "purest": "~3.1.0",
    "qs": "~6.4.0",
    "recursive-readdir": "~2.2.1",
    "redis": "~2.8.0",
    "request": "~2.83.0",
    "send": "~0.16.0",
    "server-destroy": "~1.0.1",
    "snyk": "~1.42.0",
    "through": "~2.3.8",
    "toobusy-js": "~0.5.1",
    "underscore": "~1.8.3",
    "underscore.string": "~3.3.0",
    "wildcard": "~1.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~4.1.1",
    "@commitlint/config-angular": "~3.1.1",
    "env-test": "~1.0.0",
    "husky": "~0.13.3",
    "istanbul": "^0.4.5",
    "istanbul-cobertura-badger": "^1.1.0",
    "lint-staged": "^3.4.0",
    "mocha": "~3.2.0",
    "nock": "~9.0.0",
    "nodeunit": ">=0.5.1",
    "prettier": "^1.1.0",
    "should": "~13.1.0",
    "sinon": "~4.0.2",
    "snazzy": "~7.0.0",
    "standard": "10.0.2",
    "sinon-test": "~1.0.2",
    "superagent": "~3.5.2",
    "superagent-mock": "~3.3.0",
    "supertest": "~3.0.0",
    "uuid": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dadi/web.git"
  },
  "bugs": {
    "url": "https://github.com/dadi/web/issues"
  },
  "homepage": "https://github.com/dadi/web",
  "directories": {
    "test": "test"
  },
  "standard": {
    "ignore": [
      "scripts",
      "test",
      "workspace"
    ]
  },
  "author": "DADI <team@dadi.tech>",
  "license": "SEE LICENSE IN GPL.md",
  "snyk": true
}
